    "rich-text#redes-sociais-title": {
        "props": {
            "text": "Redes Sociais",
            "blockClass": "footer-row-4"
        }
    },
    "rich-text#payments-title": {
        "props": {
            "text": "Redes Sociais",
            "blockClass": "footer-row-4"
        }
    },
    "rich-text#selos-title": {
        "props": {
            "text": "Redes Sociais",
